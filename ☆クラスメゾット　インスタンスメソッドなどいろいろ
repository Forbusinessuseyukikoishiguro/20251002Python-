        self.owner_name = owner_name
        self.stock = stock
    
    @classmethod
    def from_dict(cls, data):
        # cls() ã‚’ä½¿ã†ã“ã¨ã§ã€ç¶™æ‰¿å…ˆã§ã‚‚æ­£ã—ãå‹•ã
        return cls(data["owner"], data["stock"])


