def file_operations_demo():
    """ãƒ•ã‚¡ã‚¤ãƒ«æ“ä½œã®ãƒ‡ãƒ¢"""
    
    # ãƒ•ã‚¡ã‚¤ãƒ«ã®èª­ã¿è¾¼ã¿
    try:
        with open('data.txt', 'r', encoding='utf-8') as f:
            content = f.read()
            print(f"ãƒ•ã‚¡ã‚¤ãƒ«å†…å®¹: {content}")
    except FileNotFoundError:
        print("ãƒ•ã‚¡ã‚¤ãƒ«ãŒè¦‹ã¤ã‹ã‚Šã¾ã›ã‚“")
    
    # ãƒ•ã‚¡ã‚¤ãƒ«ã¸ã®æ›¸ãè¾¼ã¿
    with open('output.txt', 'w', encoding='utf-8') as f:
        f.write("Hello, World!\n")
        f.write("Python ãƒ•ã‚¡ã‚¤ãƒ«æ“ä½œ")
    
    # è¡Œã”ã¨ã«èª­ã¿è¾¼ã¿
    with open('output.txt', 'r', encoding='utf-8') as f:
        for line in f:
            print(line.strip())
    
    # JSONãƒ•ã‚¡ã‚¤ãƒ«ã®æ“ä½œ
    import json
    
    data = {
        "name": "ç”°ä¸­",
        "age": 30,
        "skills": ["Python", "Git", "Jenkins"]
    }
    
    # JSONæ›¸ãè¾¼ã¿
    with open('data.json', 'w', encoding='utf-8') as f:
        json.dump(data, f, ensure_ascii=False, indent=2)
    
    # JSONèª­ã¿è¾¼ã¿
    with open('data.json', 'r', encoding='utf-8') as f:
        loaded_data = json.load(f)
        print(f"JSON data: {loaded_data}")

# ===== Gitæ“ä½œï¼ˆPythonï¼‰=====
print("\nâ–  Pythonã‹ã‚‰Gitæ“ä½œ")

git_python_example = """
# GitPythonãƒ©ã‚¤ãƒ–ãƒ©ãƒªã‚’ä½¿ç”¨
# pip install GitPython

from git import Repo
import os

# ãƒªãƒã‚¸ãƒˆãƒªã®ã‚¯ãƒ­ãƒ¼ãƒ³
def clone_repository(url, path):
    '''ãƒªãƒã‚¸ãƒˆãƒªã‚’ã‚¯ãƒ­ãƒ¼ãƒ³ã™ã‚‹'''
    Repo.clone_from(url, path)
    print(f"ã‚¯ãƒ­ãƒ¼ãƒ³å®Œäº†: {path}")

# ã‚³ãƒŸãƒƒãƒˆã¨ãƒ—ãƒƒã‚·ãƒ¥
def commit_and_push(repo_path, files, message):
    '''ãƒ•ã‚¡ã‚¤ãƒ«ã‚’ã‚³ãƒŸãƒƒãƒˆã—ã¦ãƒ—ãƒƒã‚·ãƒ¥'''
    repo = Repo(repo_path)
    
    # ãƒ•ã‚¡ã‚¤ãƒ«ã‚’è¿½åŠ 
    repo.index.add(files)
    
    # ã‚³ãƒŸãƒƒãƒˆ
    repo.index.commit(message)
    
    # ãƒ—ãƒƒã‚·ãƒ¥
    origin = repo.remote('origin')
    origin.push()
    
    print(f"ã‚³ãƒŸãƒƒãƒˆï¼†ãƒ—ãƒƒã‚·ãƒ¥å®Œäº†: {message}")

# ãƒ–ãƒ©ãƒ³ãƒã®ä½œæˆã¨åˆ‡ã‚Šæ›¿ãˆ
def create_and_switch_branch(repo_path, branch_name):
    '''æ–°ã—ã„ãƒ–ãƒ©ãƒ³ãƒã‚’ä½œæˆã—ã¦åˆ‡ã‚Šæ›¿ãˆ'''
    repo = Repo(repo_path)
    
    # ãƒ–ãƒ©ãƒ³ãƒä½œæˆ
    new_branch = repo.create_head(branch_name)
    
    # ãƒ–ãƒ©ãƒ³ãƒåˆ‡ã‚Šæ›¿ãˆ
    new_branch.checkout()
    
    print(f"ãƒ–ãƒ©ãƒ³ãƒä½œæˆï¼†åˆ‡ã‚Šæ›¿ãˆ: {branch_name}")

# ä½¿ç”¨ä¾‹
# clone_repository('https://github.com/user/repo.git', './my-repo')
# commit_and_push('./my-repo', ['file.py'], 'Update file.py')
# create_and_switch_branch('./my-repo', 'feature-new')
"""

print(git_python_example)

# ===== Jenkins APIæ“ä½œ =====
print("\nâ–  Pythonã‹ã‚‰Jenkinsæ“ä½œ")

jenkins_python_example = """
# python-jenkinsãƒ©ã‚¤ãƒ–ãƒ©ãƒªã‚’ä½¿ç”¨
# pip install python-jenkins

import jenkins

# Jenkinsã‚µãƒ¼ãƒãƒ¼ã«æ¥ç¶š
server = jenkins.Jenkins(
    'http://localhost:8080',
    username='admin',
    password='admin_password'
)

# ã‚¸ãƒ§ãƒ–ã®ä¸€è¦§å–å¾—
def list_jobs():
    '''å…¨ã‚¸ãƒ§ãƒ–ã‚’ä¸€è¦§è¡¨ç¤º'''
    jobs = server.get_jobs()
    for job in jobs:
        print(f"Job: {job['name']}")

# ã‚¸ãƒ§ãƒ–ã®ãƒ“ãƒ«ãƒ‰å®Ÿè¡Œ
def trigger_build(job_name):
    '''ã‚¸ãƒ§ãƒ–ã‚’ãƒ“ãƒ«ãƒ‰'''
    server.build_job(job_name)
    print(f"ãƒ“ãƒ«ãƒ‰é–‹å§‹: {job_name}")

# ãƒ“ãƒ«ãƒ‰çŠ¶æ…‹ã®ç¢ºèª
def get_build_status(job_name, build_number):
    '''ãƒ“ãƒ«ãƒ‰çŠ¶æ…‹ã‚’ç¢ºèª'''
    build_info = server.get_build_info(job_name, build_number)
    return build_info['result']

# ã‚¸ãƒ§ãƒ–ã®ä½œæˆ
def create_job(job_name, config_xml):
    '''æ–°ã—ã„ã‚¸ãƒ§ãƒ–ã‚’ä½œæˆ'''
    server.create_job(job_name, config_xml)
    print(f"ã‚¸ãƒ§ãƒ–ä½œæˆ: {job_name}")

# ä½¿ç”¨ä¾‹
# list_jobs()
# trigger_build('my-python-project')
# status = get_build_status('my-python-project', 1)
# print(f"ãƒ“ãƒ«ãƒ‰çŠ¶æ…‹: {status}")
"""

print(jenkins_python_example)

# ==========================================================
# ã€Part 5ã€‘å®Ÿè·µçš„ãªã‚¹ã‚¯ãƒªãƒ—ãƒˆä¾‹
