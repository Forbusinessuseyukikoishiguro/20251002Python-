## 5ï¸âƒ£ å®Œå…¨å®Ÿé¨“ã‚³ãƒ¼ãƒ‰

```python
"""
ç¶™æ‰¿ã®çœŸå®Ÿã‚’ç¢ºèªã™ã‚‹å®Ÿé¨“ã‚³ãƒ¼ãƒ‰
"""

import sys


class GrandParent:
    """ç¥–çˆ¶æ¯ã‚¯ãƒ©ã‚¹"""
    
    def __init__(self):
        print(f"\nã€ç¥–çˆ¶æ¯__init__ã€‘")
        print(f"  selfã®ã‚¢ãƒ‰ãƒ¬ã‚¹: {hex(id(self))}")
        print(f"  selfã®å‹: {type(self).__name__}")
        self.ç¥–çˆ¶æ¯ã®å±æ€§ = "ç¥–çˆ¶æ¯"
    
    def method(self):
        print(f"\nã€ç¥–çˆ¶æ¯methodã€‘")
        print(f"  selfã®ã‚¢ãƒ‰ãƒ¬ã‚¹: {hex(id(self))}")
        print(f"  selfã®å‹: {type(self).__name__}")
        return "ç¥–çˆ¶æ¯"


class Parent(GrandParent):
    """è¦ªã‚¯ãƒ©ã‚¹"""
    
    def __init__(self):
        print(f"\nã€è¦ª__init__é–‹å§‹ã€‘")
        print(f"  selfã®ã‚¢ãƒ‰ãƒ¬ã‚¹: {hex(id(self))}")
        print(f"  selfã®å‹: {type(self).__name__}")
        
        super().__init__()  # â† ç¥–çˆ¶æ¯ã‚’å‘¼ã¶
        
        self.è¦ªã®å±æ€§ = "è¦ª"
        print(f"ã€è¦ª__init__å®Œäº†ã€‘")
    
    def method(self):
        print(f"\nã€è¦ªmethodé–‹å§‹ã€‘")
        print(f"  selfã®ã‚¢ãƒ‰ãƒ¬ã‚¹: {hex(id(self))}")
        print(f"  selfã®å‹: {type(self).__name__}")
        
        result = super().method()  # â† ç¥–çˆ¶æ¯ã‚’å‘¼ã¶
        
        print(f"ã€è¦ªmethodå®Œäº†ã€‘")
        return f"è¦ª+{result}"


class Child(Parent):
    """å­ã‚¯ãƒ©ã‚¹"""
    
    def __init__(self):
        print(f"\nã€å­__init__é–‹å§‹ã€‘")
        print(f"  selfã®ã‚¢ãƒ‰ãƒ¬ã‚¹: {hex(id(self))}")
        print(f"  selfã®å‹: {type(self).__name__}")
        
        super().__init__()  # â† è¦ªã‚’å‘¼ã¶
        
        self.å­ã®å±æ€§ = "å­"
        print(f"ã€å­__init__å®Œäº†ã€‘")
    
    def method(self):
        print(f"\nã€å­methodé–‹å§‹ã€‘")
        print(f"  selfã®ã‚¢ãƒ‰ãƒ¬ã‚¹: {hex(id(self))}")
        print(f"  selfã®å‹: {type(self).__name__}")
        
        result = super().method()  # â† è¦ªã‚’å‘¼ã¶
        
        print(f"ã€å­methodå®Œäº†ã€‘")
        return f"å­+{result}"


# ========================================
# å®Ÿé¨“1: ã‚¤ãƒ³ã‚¹ã‚¿ãƒ³ã‚¹ä½œæˆ
# ========================================

print("=" * 60)
print("å®Ÿé¨“1: ã‚¤ãƒ³ã‚¹ã‚¿ãƒ³ã‚¹ä½œæˆï¼ˆã‚¢ãƒ‰ãƒ¬ã‚¹ã‚’è¿½è·¡ï¼‰")
print("=" * 60)

child = Child()

print("\n" + "=" * 60)
print("ä½œæˆã•ã‚ŒãŸã‚¤ãƒ³ã‚¹ã‚¿ãƒ³ã‚¹ã®æƒ…å ±")
print("=" * 60)
print(f"childã®ã‚¢ãƒ‰ãƒ¬ã‚¹: {hex(id(child))}")
print(f"childã®å‹: {type(child).__name__}")
print(f"å±æ€§: {child.__dict__}")


# ========================================
# å®Ÿé¨“2: ãƒ¡ã‚½ãƒƒãƒ‰å‘¼ã³å‡ºã—
# ========================================

print("\n\n" + "=" * 60)
print("å®Ÿé¨“2: ãƒ¡ã‚½ãƒƒãƒ‰å‘¼ã³å‡ºã—ï¼ˆã‚¢ãƒ‰ãƒ¬ã‚¹ã‚’è¿½è·¡ï¼‰")
print("=" * 60)

result = child.method()

print("\n" + "=" * 60)
print("çµæœ")
print("=" * 60)
print(f"è¿”ã‚Šå€¤: {result}")


# ========================================
# å®Ÿé¨“3: MROç¢ºèª
# ========================================

print("\n\n" + "=" * 60)
print("å®Ÿé¨“3: ãƒ¡ã‚½ãƒƒãƒ‰è§£æ±ºé †åºï¼ˆMROï¼‰")
print("=" * 60)

print("Childã‚¯ãƒ©ã‚¹ã® MRO:")
for i, cls in enumerate(Child.mro(), 1):
    print(f"  {i}. {cls.__name__}")


# ========================================
# å®Ÿé¨“4: ãƒ¡ã‚½ãƒƒãƒ‰ã®å®Ÿä½“
# ========================================

print("\n\n" + "=" * 60)
print("å®Ÿé¨“4: ãƒ¡ã‚½ãƒƒãƒ‰ã‚ªãƒ–ã‚¸ã‚§ã‚¯ãƒˆ")
print("=" * 60)

print(f"child.method: {child.method}")
print(f"  __self__: {child.method.__self__}")  # â† bind ã•ã‚ŒãŸã‚¤ãƒ³ã‚¹ã‚¿ãƒ³ã‚¹
print(f"  __func__: {child.method.__func__}")  # â† å…ƒã®é–¢æ•°

print(f"\nChild.method: {Child.method}")
print(f"  ã“ã‚Œã¯é–¢æ•°: {type(Child.method)}")


# ========================================
# å®Ÿé¨“5: åŒä¸€æ€§ç¢ºèª
# ========================================

print("\n\n" + "=" * 60)
print("å®Ÿé¨“5: self ã®åŒä¸€æ€§")
print("=" * 60)

class TrackingSelf:
    """selfã‚’è¿½è·¡ã™ã‚‹ã‚¯ãƒ©ã‚¹"""
    
    def __init__(self):
        print(f"init: self={hex(id(self))}")
        self.init_self_id = id(self)
    
    def method(self):
        print(f"method: self={hex(id(self))}")
        print(f"  initã¨åŒã˜ï¼Ÿ: {id(self) == self.init_self_id}")

obj = TrackingSelf()
obj.method()


# ========================================
# å®Ÿé¨“6: super() ã®æ­£ä½“
# ========================================

print("\n\n" + "=" * 60)
print("å®Ÿé¨“6: super() ã¯ä½•ã‚’è¿”ã™ï¼Ÿ")
print("=" * 60)

class Parent:
    def method(self):
        return "è¦ª"

class Child(Parent):
    def method(self):
        print(f"super()ã®å‹: {type(super())}")
        print(f"super()ã®å€¤: {super()}")
        return super().method()

child = Child()
child.method()
```

