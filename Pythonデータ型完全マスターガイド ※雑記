"""
ã€Pythonã®ä¸»è¦ãªãƒ‡ãƒ¼ã‚¿å‹ã€‘

åŸºæœ¬å‹ï¼š
  int       æ•´æ•°
  float     å°æ•°
  str       æ–‡å­—åˆ—
  bool      çœŸå½å€¤

ã‚³ãƒ¬ã‚¯ã‚·ãƒ§ãƒ³å‹ï¼š
  list      ãƒªã‚¹ãƒˆï¼ˆå¯å¤‰ã€é †åºã‚ã‚Šï¼‰
  tuple     ã‚¿ãƒ—ãƒ«ï¼ˆä¸å¤‰ã€é †åºã‚ã‚Šï¼‰
  set       ã‚»ãƒƒãƒˆï¼ˆå¯å¤‰ã€é †åºãªã—ã€é‡è¤‡ãªã—ï¼‰
  dict      è¾æ›¸ï¼ˆå¯å¤‰ã€ã‚­ãƒ¼ãƒ»ãƒãƒªãƒ¥ãƒ¼ã®ãƒšã‚¢ï¼‰

ç‰¹æ®Šå‹ï¼š
  None      å€¤ãŒå­˜åœ¨ã—ãªã„ã“ã¨ã‚’è¡¨ã™
"""

print("=" * 70)
print("Pythonãƒ‡ãƒ¼ã‚¿å‹å®Œå…¨ãƒã‚¹ã‚¿ãƒ¼ã‚¬ã‚¤ãƒ‰")
print("=" * 70)

# ==========================================================
# ã€1ã€‘åŸºæœ¬å‹ï¼ˆãƒ—ãƒªãƒŸãƒ†ã‚£ãƒ–å‹ï¼‰
# ==========================================================

print("\n" + "=" * 70)
print("ã€1ã€‘åŸºæœ¬å‹ - int, float, str, bool")
print("=" * 70)

# --- intï¼ˆæ•´æ•°ï¼‰---
print("\nâ–  intï¼ˆæ•´æ•°å‹ï¼‰")
age = 25  # æ•´æ•°ã‚’ä»£å…¥
count = -10  # è² ã®æ•´æ•°ã‚‚å¯èƒ½
big_number = 1000000  # å¤§ããªæ•°å€¤ã‚‚æ‰±ãˆã‚‹

print(f"å¹´é½¢: {age}, å‹: {type(age)}")  # å‹ã‚’ç¢ºèª
print(f"è¨ˆç®—: {age + 5}")  # è¶³ã—ç®—
print(f"è¨ˆç®—: {age * 2}")  # æ›ã‘ç®—
print(f"è¨ˆç®—: {age // 3}")  # æ•´æ•°é™¤ç®—ï¼ˆå°æ•°ç‚¹ä»¥ä¸‹åˆ‡ã‚Šæ¨ã¦ï¼‰
print(f"è¨ˆç®—: {age % 3}")  # ä½™ã‚Š

# --- floatï¼ˆå°æ•°ï¼‰---
print("\nâ–  floatï¼ˆæµ®å‹•å°æ•°ç‚¹å‹ï¼‰")
price = 99.99  # å°æ•°ã‚’ä»£å…¥
temperature = -5.5  # è² ã®å°æ•°ã‚‚å¯èƒ½

print(f"ä¾¡æ ¼: {price}, å‹: {type(price)}")
print(f"è¨ˆç®—: {price * 1.1}")  # å°æ•°ã®è¨ˆç®—
print(f"å°æ•°ç‚¹2æ¡: {price:.2f}")  # ãƒ•ã‚©ãƒ¼ãƒãƒƒãƒˆ
print(f"æ•´æ•°ã«å¤‰æ›: {int(price)}")  # å‹å¤‰æ›

# --- strï¼ˆæ–‡å­—åˆ—ï¼‰---
print("\nâ–  strï¼ˆæ–‡å­—åˆ—å‹ï¼‰")
name = "å±±ç”°å¤ªéƒ"  # æ–‡å­—åˆ—ã‚’ä»£å…¥
message = 'ã“ã‚“ã«ã¡ã¯'  # ã‚·ãƒ³ã‚°ãƒ«ã‚¯ã‚©ãƒ¼ãƒˆã‚‚å¯èƒ½
multi_line = """ã“ã‚Œã¯
è¤‡æ•°è¡Œã®
æ–‡å­—åˆ—ã§ã™"""  # è¤‡æ•°è¡Œã®æ–‡å­—åˆ—

print(f"åå‰: {name}, å‹: {type(name)}")
print(f"é•·ã•: {len(name)}")  # æ–‡å­—æ•°ã‚’å–å¾—
print(f"å¤§æ–‡å­—: {'hello'.upper()}")  # å¤§æ–‡å­—ã«å¤‰æ›
print(f"çµåˆ: {name + 'ã•ã‚“'}")  # æ–‡å­—åˆ—ã®çµåˆ
print(f"åˆ†å‰²: {'a,b,c'.split(',')}")  # ã‚«ãƒ³ãƒã§åˆ†å‰²
print(f"ç½®æ›: {name.replace('å¤ªéƒ', 'èŠ±å­')}")  # ç½®æ›

# --- boolï¼ˆçœŸå½å€¤ï¼‰---
print("\nâ–  boolï¼ˆãƒ–ãƒ¼ãƒ«å‹ï¼‰")
is_student = True  # çœŸ
is_working = False  # å½

print(f"å­¦ç”Ÿï¼Ÿ: {is_student}, å‹: {type(is_student)}")
print(f"è«–ç†æ¼”ç®—: {is_student and is_working}")  # AND
print(f"è«–ç†æ¼”ç®—: {is_student or is_working}")  # OR
print(f"è«–ç†æ¼”ç®—: {not is_student}")  # NOT

# --- Noneï¼ˆå€¤ãªã—ï¼‰---
print("\nâ–  Noneï¼ˆå€¤ãŒå­˜åœ¨ã—ãªã„ï¼‰")
result = None  # å€¤ãŒå­˜åœ¨ã—ãªã„çŠ¶æ…‹

print(f"çµæœ: {result}, å‹: {type(result)}")
if result is None:  # Noneã®ãƒã‚§ãƒƒã‚¯ã¯ is ã‚’ä½¿ã†
    print("çµæœãŒã‚ã‚Šã¾ã›ã‚“")

# ==========================================================
# ã€2ã€‘ãƒªã‚¹ãƒˆï¼ˆlistï¼‰- æœ€ã‚‚ã‚ˆãä½¿ã†ï¼
# ==========================================================

print("\n" + "=" * 70)
print("ã€2ã€‘ãƒªã‚¹ãƒˆï¼ˆlistï¼‰- é †åºã‚ã‚Šã€å¯å¤‰ã€é‡è¤‡OK")
print("=" * 70)

# --- åŸºæœ¬çš„ãªä½¿ã„æ–¹ ---
print("\nâ–  åŸºæœ¬æ“ä½œ")
fruits = ["ã‚Šã‚“ã”", "ãƒãƒŠãƒŠ", "ã‚ªãƒ¬ãƒ³ã‚¸"]  # ãƒªã‚¹ãƒˆã®ä½œæˆ
numbers = [1, 2, 3, 4, 5]  # æ•°å€¤ã®ãƒªã‚¹ãƒˆ
mixed = [1, "hello", 3.14, True]  # ç•°ãªã‚‹å‹ã‚’æ··åœ¨å¯èƒ½

print(f"ãƒªã‚¹ãƒˆ: {fruits}")
print(f"é•·ã•: {len(fruits)}")  # è¦ç´ æ•°
print(f"æœ€åˆã®è¦ç´ : {fruits[0]}")  # ã‚¤ãƒ³ãƒ‡ãƒƒã‚¯ã‚¹ã§ã‚¢ã‚¯ã‚»ã‚¹ï¼ˆ0å§‹ã¾ã‚Šï¼‰
print(f"æœ€å¾Œã®è¦ç´ : {fruits[-1]}")  # è² ã®ã‚¤ãƒ³ãƒ‡ãƒƒã‚¯ã‚¹ï¼ˆæœ€å¾Œã‹ã‚‰ï¼‰
print(f"ã‚¹ãƒ©ã‚¤ã‚¹: {numbers[1:4]}")  # ç¯„å›²å–å¾—ï¼ˆ1ç•ªç›®ã‹ã‚‰3ç•ªç›®ã¾ã§ï¼‰

# --- è¦ç´ ã®è¿½åŠ  ---
print("\nâ–  è¦ç´ ã®è¿½åŠ ")
fruits.append("ã¶ã©ã†")  # æœ«å°¾ã«è¿½åŠ 
print(f"appendå¾Œ: {fruits}")

fruits.insert(1, "ã„ã¡ã”")  # æŒ‡å®šä½ç½®ã«æŒ¿å…¥
print(f"insertå¾Œ: {fruits}")

fruits.extend(["ãƒ¡ãƒ­ãƒ³", "ã™ã„ã‹"])  # è¤‡æ•°è¿½åŠ 
print(f"extendå¾Œ: {fruits}")

# --- è¦ç´ ã®å‰Šé™¤ ---
print("\nâ–  è¦ç´ ã®å‰Šé™¤")
fruits.remove("ãƒãƒŠãƒŠ")  # å€¤ã§å‰Šé™¤ï¼ˆæœ€åˆã®ä¸€è‡´ï¼‰
print(f"removeå¾Œ: {fruits}")

removed = fruits.pop()  # æœ«å°¾ã‚’å‰Šé™¤ã—ã¦è¿”ã™
print(f"popå¾Œ: {fruits}, å‰Šé™¤ã•ã‚ŒãŸ: {removed}")

removed = fruits.pop(0)  # æŒ‡å®šä½ç½®ã‚’å‰Šé™¤
print(f"pop(0)å¾Œ: {fruits}, å‰Šé™¤ã•ã‚ŒãŸ: {removed}")

del fruits[0]  # ã‚¤ãƒ³ãƒ‡ãƒƒã‚¯ã‚¹ã§å‰Šé™¤
print(f"delå¾Œ: {fruits}")

fruits.clear()  # å…¨å‰Šé™¤
print(f"clearå¾Œ: {fruits}")

# --- ä¾¿åˆ©ãªæ“ä½œ ---
print("\nâ–  ä¾¿åˆ©ãªæ“ä½œ")
numbers = [3, 1, 4, 1, 5, 9, 2, 6]
print(f"å…ƒã®ãƒªã‚¹ãƒˆ: {numbers}")
print(f"ã‚½ãƒ¼ãƒˆ: {sorted(numbers)}")  # ã‚½ãƒ¼ãƒˆï¼ˆå…ƒã¯å¤‰æ›´ã—ãªã„ï¼‰
numbers.sort()  # ã‚½ãƒ¼ãƒˆï¼ˆå…ƒã‚’å¤‰æ›´ï¼‰
print(f"sortå¾Œ: {numbers}")
numbers.reverse()  # é€†é †
print(f"reverseå¾Œ: {numbers}")
print(f"æœ€å¤§å€¤: {max(numbers)}")  # æœ€å¤§
print(f"æœ€å°å€¤: {min(numbers)}")  # æœ€å°
print(f"åˆè¨ˆ: {sum(numbers)}")  # åˆè¨ˆ
print(f"ã‚«ã‚¦ãƒ³ãƒˆ: {numbers.count(1)}")  # ç‰¹å®šã®å€¤ã®å€‹æ•°
print(f"ã‚¤ãƒ³ãƒ‡ãƒƒã‚¯ã‚¹: {numbers.index(9)}")  # å€¤ã®ä½ç½®

# --- ãƒªã‚¹ãƒˆå†…åŒ…è¡¨è¨˜ï¼ˆè¶…é‡è¦ï¼ï¼‰---
print("\nâ–  ãƒªã‚¹ãƒˆå†…åŒ…è¡¨è¨˜")
squares = [x**2 for x in range(5)]  # 0-4ã®2ä¹—
print(f"2ä¹—ã®ãƒªã‚¹ãƒˆ: {squares}")

evens = [x for x in range(10) if x % 2 == 0]  # å¶æ•°ã®ã¿
print(f"å¶æ•°ã®ãƒªã‚¹ãƒˆ: {evens}")

# ==========================================================
# ã€3ã€‘ã‚¿ãƒ—ãƒ«ï¼ˆtupleï¼‰- å¤‰æ›´ä¸å¯ã®ãƒªã‚¹ãƒˆ
# ==========================================================

print("\n" + "=" * 70)
print("ã€3ã€‘ã‚¿ãƒ—ãƒ«ï¼ˆtupleï¼‰- é †åºã‚ã‚Šã€ä¸å¤‰ã€é‡è¤‡OK")
