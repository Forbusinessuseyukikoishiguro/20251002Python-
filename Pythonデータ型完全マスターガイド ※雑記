print(f"ç·¯åº¦: {coordinates[0]}")  # ã‚¤ãƒ³ãƒ‡ãƒƒã‚¯ã‚¹ã§ã‚¢ã‚¯ã‚»ã‚¹
print(f"çµŒåº¦: {coordinates[1]}")

# ã‚¿ãƒ—ãƒ«ã¯å¤‰æ›´ã§ããªã„
# coordinates[0] = 40  # ã‚¨ãƒ©ãƒ¼ï¼TypeError

# --- ã‚¿ãƒ—ãƒ«ã®ã‚¢ãƒ³ãƒ‘ãƒƒã‚¯ ---
print("\nâ–  ã‚¢ãƒ³ãƒ‘ãƒƒã‚¯ï¼ˆåˆ†è§£ä»£å…¥ï¼‰")
lat, lon = coordinates  # ã‚¿ãƒ—ãƒ«ã‚’å¤‰æ•°ã«åˆ†è§£
print(f"ç·¯åº¦: {lat}, çµŒåº¦: {lon}")

r, g, b = rgb  # RGBå€¤ã‚’åˆ†è§£
print(f"R: {r}, G: {g}, B: {b}")

# --- è¤‡æ•°ã®å€¤ã‚’è¿”ã™é–¢æ•°ã§ã‚ˆãä½¿ã† ---
print("\nâ–  é–¢æ•°ã§ã®ä½¿ç”¨ä¾‹")
def get_min_max(numbers):
    """æœ€å°å€¤ã¨æœ€å¤§å€¤ã‚’ã‚¿ãƒ—ãƒ«ã§è¿”ã™"""
    return min(numbers), max(numbers)  # ã‚¿ãƒ—ãƒ«ã§è¿”ã™

min_val, max_val = get_min_max([1, 5, 3, 9, 2])
print(f"æœ€å°: {min_val}, æœ€å¤§: {max_val}")

# ==========================================================
# ã€4ã€‘ã‚»ãƒƒãƒˆï¼ˆsetï¼‰- é‡è¤‡ãªã—ã€é †åºãªã—
# ==========================================================

print("\n" + "=" * 70)
print("ã€4ã€‘ã‚»ãƒƒãƒˆï¼ˆsetï¼‰- é †åºãªã—ã€å¯å¤‰ã€é‡è¤‡ãªã—")
print("=" * 70)

# --- åŸºæœ¬çš„ãªä½¿ã„æ–¹ ---
print("\nâ–  åŸºæœ¬æ“ä½œ")
unique_numbers = {1, 2, 3, 4, 5}  # ã‚»ãƒƒãƒˆã®ä½œæˆ
colors = {"èµ¤", "é’", "ç·‘"}

print(f"ã‚»ãƒƒãƒˆ: {unique_numbers}")
print(f"é•·ã•: {len(unique_numbers)}")

# é‡è¤‡ã¯è‡ªå‹•çš„ã«å‰Šé™¤ã•ã‚Œã‚‹
duplicates = {1, 2, 2, 3, 3, 3}
print(f"é‡è¤‡å‰Šé™¤: {duplicates}")  # {1, 2, 3}

# ãƒªã‚¹ãƒˆã‹ã‚‰é‡è¤‡ã‚’å‰Šé™¤
numbers_list = [1, 2, 2, 3, 3, 3, 4, 4]
unique = list(set(numbers_list))  # ãƒªã‚¹ãƒˆâ†’ã‚»ãƒƒãƒˆâ†’ãƒªã‚¹ãƒˆ
print(f"é‡è¤‡å‰Šé™¤ã•ã‚ŒãŸãƒªã‚¹ãƒˆ: {unique}")

# --- è¦ç´ ã®è¿½åŠ ãƒ»å‰Šé™¤ ---
print("\nâ–  è¦ç´ ã®è¿½åŠ ãƒ»å‰Šé™¤")
colors.add("é»„è‰²")  # è¦ç´ ã‚’è¿½åŠ 
print(f"addå¾Œ: {colors}")

colors.remove("é’")  # è¦ç´ ã‚’å‰Šé™¤ï¼ˆå­˜åœ¨ã—ãªã„ã¨ã‚¨ãƒ©ãƒ¼ï¼‰
print(f"removeå¾Œ: {colors}")

colors.discard("ç´«")  # è¦ç´ ã‚’å‰Šé™¤ï¼ˆå­˜åœ¨ã—ãªãã¦ã‚‚ã‚¨ãƒ©ãƒ¼ãªã—ï¼‰
print(f"discardå¾Œ: {colors}")

# --- é›†åˆæ¼”ç®—ï¼ˆæ•°å­¦çš„ãªæ¼”ç®—ï¼‰---
print("\nâ–  é›†åˆæ¼”ç®—")
set_a = {1, 2, 3, 4, 5}
set_b = {4, 5, 6, 7, 8}

print(f"A: {set_a}")
print(f"B: {set_b}")
print(f"å’Œé›†åˆï¼ˆAâˆªBï¼‰: {set_a | set_b}")  # ã©ã¡ã‚‰ã‹ã«ã‚ã‚‹
print(f"ç©é›†åˆï¼ˆAâˆ©Bï¼‰: {set_a & set_b}")  # ä¸¡æ–¹ã«ã‚ã‚‹
print(f"å·®é›†åˆï¼ˆA-Bï¼‰: {set_a - set_b}")  # Aã«ã‚ã£ã¦ã€Bã«ãªã„
print(f"å¯¾ç§°å·®ï¼ˆAâŠ•Bï¼‰: {set_a ^ set_b}")  # ã©ã¡ã‚‰ã‹ç‰‡æ–¹ã ã‘ã«ã‚ã‚‹

# --- å®Ÿç”¨ä¾‹ï¼šã‚¿ã‚°ã®ç®¡ç† ---
print("\nâ–  å®Ÿç”¨ä¾‹ï¼šã‚¿ã‚°")
post1_tags = {"Python", "ãƒ—ãƒ­ã‚°ãƒ©ãƒŸãƒ³ã‚°", "åˆå¿ƒè€…"}
post2_tags = {"Python", "æ©Ÿæ¢°å­¦ç¿’", "AI"}

common = post1_tags & post2_tags  # å…±é€šã‚¿ã‚°
all_tags = post1_tags | post2_tags  # å…¨ã‚¿ã‚°
print(f"å…±é€šã‚¿ã‚°: {common}")
print(f"å…¨ã‚¿ã‚°: {all_tags}")

# ==========================================================
# ã€5ã€‘è¾æ›¸ï¼ˆdictï¼‰- ã‚­ãƒ¼ãƒ»ãƒãƒªãƒ¥ãƒ¼ã®ãƒšã‚¢
# ==========================================================

print("\n" + "=" * 70)
print("ã€5ã€‘è¾æ›¸ï¼ˆdictï¼‰- ã‚­ãƒ¼ãƒ»ãƒãƒªãƒ¥ãƒ¼ã®ãƒšã‚¢ã€é †åºã‚ã‚Šâ€»")
print("â€»Python 3.7ä»¥é™ã¯æŒ¿å…¥é †ã‚’ä¿æŒ")
