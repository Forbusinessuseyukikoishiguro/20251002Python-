# --- åŸºæœ¬çš„ãªä½¿ã„æ–¹ ---
print("\nâ–  åŸºæœ¬æ“ä½œ")
user = {
    "name": "å±±ç”°å¤ªéƒ",
    "age": 30,
    "email": "yamada@example.com",
    "is_active": True
}

print(f"è¾æ›¸: {user}")
print(f"åå‰: {user['name']}")  # ã‚­ãƒ¼ã§å€¤ã‚’å–å¾—
print(f"å¹´é½¢: {user.get('age')}")  # getãƒ¡ã‚½ãƒƒãƒ‰ï¼ˆå®‰å…¨ï¼‰
print(f"å­˜åœ¨ã—ãªã„ã‚­ãƒ¼: {user.get('phone', 'ãªã—')}")  # ãƒ‡ãƒ•ã‚©ãƒ«ãƒˆå€¤

# --- è¦ç´ ã®è¿½åŠ ãƒ»æ›´æ–° ---
print("\nâ–  è¦ç´ ã®è¿½åŠ ãƒ»æ›´æ–°")
user["phone"] = "090-1234-5678"  # æ–°ã—ã„ã‚­ãƒ¼ã‚’è¿½åŠ 
print(f"è¿½åŠ å¾Œ: {user}")

user["age"] = 31  # æ—¢å­˜ã®ã‚­ãƒ¼ã‚’æ›´æ–°
print(f"æ›´æ–°å¾Œ: {user}")

user.update({"city": "æ±äº¬", "country": "æ—¥æœ¬"})  # è¤‡æ•°è¿½åŠ 
print(f"updateå¾Œ: {user}")

# --- è¦ç´ ã®å‰Šé™¤ ---
print("\nâ–  è¦ç´ ã®å‰Šé™¤")
del user["phone"]  # ã‚­ãƒ¼ã§å‰Šé™¤
print(f"delå¾Œ: {user}")

removed = user.pop("email")  # ã‚­ãƒ¼ã§å‰Šé™¤ã—ã¦å€¤ã‚’è¿”ã™
print(f"popå¾Œ: {user}, å‰Šé™¤ã•ã‚ŒãŸ: {removed}")

# --- ä¾¿åˆ©ãªæ“ä½œ ---
print("\nâ–  ä¾¿åˆ©ãªæ“ä½œ")
print(f"ã‚­ãƒ¼ä¸€è¦§: {list(user.keys())}")  # å…¨ã‚­ãƒ¼
print(f"å€¤ä¸€è¦§: {list(user.values())}")  # å…¨å€¤
print(f"ãƒšã‚¢ä¸€è¦§: {list(user.items())}")  # ã‚­ãƒ¼ãƒ»å€¤ã®ãƒšã‚¢

print(f"ã‚­ãƒ¼ã®å­˜åœ¨ç¢ºèª: {'name' in user}")  # ã‚­ãƒ¼ãŒã‚ã‚‹ã‹
print(f"å€¤ã®å­˜åœ¨ç¢ºèª: {'å±±ç”°å¤ªéƒ' in user.values()}")  # å€¤ãŒã‚ã‚‹ã‹

# --- ãƒ«ãƒ¼ãƒ—å‡¦ç† ---
print("\nâ–  ãƒ«ãƒ¼ãƒ—å‡¦ç†")
for key in user:  # ã‚­ãƒ¼ã§ãƒ«ãƒ¼ãƒ—
    print(f"{key}: {user[key]}")

print()
for key, value in user.items():  # ã‚­ãƒ¼ã¨å€¤ã§ãƒ«ãƒ¼ãƒ—
    print(f"{key} = {value}")

# --- ãƒã‚¹ãƒˆã—ãŸè¾æ›¸ ---
print("\nâ–  ãƒã‚¹ãƒˆã—ãŸè¾æ›¸")
company = {
    "name": "ABCæ ªå¼ä¼šç¤¾",
    "employees": [
        {"name": "ç”°ä¸­", "age": 25},
        {"name": "ä½è—¤", "age": 30},
        {"name": "éˆ´æœ¨", "age": 28}
    ],
