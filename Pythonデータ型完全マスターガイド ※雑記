# --- åŸºæœ¬çš„ãªä½¿ã„æ–¹ ---
print("\nâ–  åŸºæœ¬æ“ä½œ")
fruits = ["ã‚Šã‚“ã”", "ãƒãƒŠãƒŠ", "ã‚ªãƒ¬ãƒ³ã‚¸"]  # ãƒªã‚¹ãƒˆã®ä½œæˆ
numbers = [1, 2, 3, 4, 5]  # æ•°å€¤ã®ãƒªã‚¹ãƒˆ
mixed = [1, "hello", 3.14, True]  # ç•°ãªã‚‹å‹ã‚’æ··åœ¨å¯èƒ½

print(f"ãƒªã‚¹ãƒˆ: {fruits}")
print(f"é•·ã•: {len(fruits)}")  # è¦ç´ æ•°
print(f"æœ€åˆã®è¦ç´ : {fruits[0]}")  # ã‚¤ãƒ³ãƒ‡ãƒƒã‚¯ã‚¹ã§ã‚¢ã‚¯ã‚»ã‚¹ï¼ˆ0å§‹ã¾ã‚Šï¼‰
print(f"æœ€å¾Œã®è¦ç´ : {fruits[-1]}")  # è² ã®ã‚¤ãƒ³ãƒ‡ãƒƒã‚¯ã‚¹ï¼ˆæœ€å¾Œã‹ã‚‰ï¼‰
print(f"ã‚¹ãƒ©ã‚¤ã‚¹: {numbers[1:4]}")  # ç¯„å›²å–å¾—ï¼ˆ1ç•ªç›®ã‹ã‚‰3ç•ªç›®ã¾ã§ï¼‰

# --- è¦ç´ ã®è¿½åŠ  ---
print("\nâ–  è¦ç´ ã®è¿½åŠ ")
fruits.append("ã¶ã©ã†")  # æœ«å°¾ã«è¿½åŠ 
print(f"appendå¾Œ: {fruits}")

fruits.insert(1, "ã„ã¡ã”")  # æŒ‡å®šä½ç½®ã«æŒ¿å…¥
print(f"insertå¾Œ: {fruits}")

fruits.extend(["ãƒ¡ãƒ­ãƒ³", "ã™ã„ã‹"])  # è¤‡æ•°è¿½åŠ 
print(f"extendå¾Œ: {fruits}")

# --- è¦ç´ ã®å‰Šé™¤ ---
print("\nâ–  è¦ç´ ã®å‰Šé™¤")
fruits.remove("ãƒãƒŠãƒŠ")  # å€¤ã§å‰Šé™¤ï¼ˆæœ€åˆã®ä¸€è‡´ï¼‰
print(f"removeå¾Œ: {fruits}")

removed = fruits.pop()  # æœ«å°¾ã‚’å‰Šé™¤ã—ã¦è¿”ã™
print(f"popå¾Œ: {fruits}, å‰Šé™¤ã•ã‚ŒãŸ: {removed}")

removed = fruits.pop(0)  # æŒ‡å®šä½ç½®ã‚’å‰Šé™¤
print(f"pop(0)å¾Œ: {fruits}, å‰Šé™¤ã•ã‚ŒãŸ: {removed}")

del fruits[0]  # ã‚¤ãƒ³ãƒ‡ãƒƒã‚¯ã‚¹ã§å‰Šé™¤
print(f"delå¾Œ: {fruits}")

fruits.clear()  # å…¨å‰Šé™¤
print(f"clearå¾Œ: {fruits}")

# --- ä¾¿åˆ©ãªæ“ä½œ ---
print("\nâ–  ä¾¿åˆ©ãªæ“ä½œ")
numbers = [3, 1, 4, 1, 5, 9, 2, 6]
print(f"å…ƒã®ãƒªã‚¹ãƒˆ: {numbers}")
print(f"ã‚½ãƒ¼ãƒˆ: {sorted(numbers)}")  # ã‚½ãƒ¼ãƒˆï¼ˆå…ƒã¯å¤‰æ›´ã—ãªã„ï¼‰
numbers.sort()  # ã‚½ãƒ¼ãƒˆï¼ˆå…ƒã‚’å¤‰æ›´ï¼‰
print(f"sortå¾Œ: {numbers}")
numbers.reverse()  # é€†é †
print(f"reverseå¾Œ: {numbers}")
print(f"æœ€å¤§å€¤: {max(numbers)}")  # æœ€å¤§
print(f"æœ€å°å€¤: {min(numbers)}")  # æœ€å°
print(f"åˆè¨ˆ: {sum(numbers)}")  # åˆè¨ˆ
print(f"ã‚«ã‚¦ãƒ³ãƒˆ: {numbers.count(1)}")  # ç‰¹å®šã®å€¤ã®å€‹æ•°
print(f"ã‚¤ãƒ³ãƒ‡ãƒƒã‚¯ã‚¹: {numbers.index(9)}")  # å€¤ã®ä½ç½®

# --- ãƒªã‚¹ãƒˆå†…åŒ…è¡¨è¨˜ï¼ˆè¶…é‡è¦ï¼ï¼‰---
print("\nâ–  ãƒªã‚¹ãƒˆå†…åŒ…è¡¨è¨˜")
squares = [x**2 for x in range(5)]  # 0-4ã®2ä¹—
print(f"2ä¹—ã®ãƒªã‚¹ãƒˆ: {squares}")

evens = [x for x in range(10) if x % 2 == 0]  # å¶æ•°ã®ã¿
print(f"å¶æ•°ã®ãƒªã‚¹ãƒˆ: {evens}")

