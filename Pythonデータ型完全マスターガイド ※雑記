user["age"] = 31  # æ—¢å­˜ã®ã‚­ãƒ¼ã‚’æ›´æ–°
print(f"æ›´æ–°å¾Œ: {user}")

user.update({"city": "æ±äº¬", "country": "æ—¥æœ¬"})  # è¤‡æ•°è¿½åŠ 
print(f"updateå¾Œ: {user}")

# --- è¦ç´ ã®å‰Šé™¤ ---
print("\nâ–  è¦ç´ ã®å‰Šé™¤")
del user["phone"]  # ã‚­ãƒ¼ã§å‰Šé™¤
print(f"delå¾Œ: {user}")

removed = user.pop("email")  # ã‚­ãƒ¼ã§å‰Šé™¤ã—ã¦å€¤ã‚’è¿”ã™
print(f"popå¾Œ: {user}, å‰Šé™¤ã•ã‚ŒãŸ: {removed}")

# --- ä¾¿åˆ©ãªæ“ä½œ ---
print("\nâ–  ä¾¿åˆ©ãªæ“ä½œ")
