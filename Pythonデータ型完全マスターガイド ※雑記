print(f"removeå¾Œ: {colors}")

colors.discard("ç´«")  # è¦ç´ ã‚’å‰Šé™¤ï¼ˆå­˜åœ¨ã—ãªãã¦ã‚‚ã‚¨ãƒ©ãƒ¼ãªã—ï¼‰
print(f"discardå¾Œ: {colors}")

# --- é›†åˆæ¼”ç®—ï¼ˆæ•°å­¦çš„ãªæ¼”ç®—ï¼‰---
print("\nâ–  é›†åˆæ¼”ç®—")
set_a = {1, 2, 3, 4, 5}
set_b = {4, 5, 6, 7, 8}

print(f"A: {set_a}")
print(f"B: {set_b}")
print(f"å’Œé›†åˆï¼ˆAâˆªBï¼‰: {set_a | set_b}")  # ã©ã¡ã‚‰ã‹ã«ã‚ã‚‹
print(f"ç©é›†åˆï¼ˆAâˆ©Bï¼‰: {set_a & set_b}")  # ä¸¡æ–¹ã«ã‚ã‚‹
print(f"å·®é›†åˆï¼ˆA-Bï¼‰: {set_a - set_b}")  # Aã«ã‚ã£ã¦ã€Bã«ãªã„
print(f"å¯¾ç§°å·®ï¼ˆAâŠ•Bï¼‰: {set_a ^ set_b}")  # ã©ã¡ã‚‰ã‹ç‰‡æ–¹ã ã‘ã«ã‚ã‚‹

# --- å®Ÿç”¨ä¾‹ï¼šã‚¿ã‚°ã®ç®¡ç† ---
print("\nâ–  å®Ÿç”¨ä¾‹ï¼šã‚¿ã‚°")
post1_tags = {"Python", "ãƒ—ãƒ­ã‚°ãƒ©ãƒŸãƒ³ã‚°", "åˆå¿ƒè€…"}
post2_tags = {"Python", "æ©Ÿæ¢°å­¦ç¿’", "AI"}

